{
    "name": "Site Blocker",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "This extension will block the sites that are included in the javascript code",
    "action":{
        "default_popup":"popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ],
    "icons":{
        "128": "stop.png"    
    },
    "permissions":["activeTab"]
}
